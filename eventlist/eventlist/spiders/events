  $(document).ready(function() {
//jquery.fittext.js
    (function(e) {
      e.fn.fitText = function(t, n) {
        var r = t || 1, i = e.extend({minFontSize: Number.NEGATIVE_INFINITY, maxFontSize: Number.POSITIVE_INFINITY}, n);
        return this.each(function()
        {
          var t = e(this);
          var n = function()
          {
            t.css("font-size", Math.max(Math.min(t.width() / (r * 10), parseFloat(i.maxFontSize)), parseFloat(i.minFontSize)))
          };
          n();
          e(window).on("resize.fittext orientationchange.fittext", n)
        })
      }
    })(jQuery)
  });


//<!-- Accordion Magic Below -->
  $(document).ready(function()
  {
    $(".collapse2").collapse();
    $('.accordion-body').on('show', function(e)
    {
      $(e.currentTarget).parent().find('.accordion-heading').addClass('active');
      $(e.currentTarget).parent().find('.fa-angle-down').removeClass('fa-angle-down').addClass('fa-angle-up');
    });
    $('.accordion-body').on('hide', function(e)
    {
      $(e.currentTarget).parent().find('.accordion-heading').removeClass('active');
      $(e.currentTarget).parent().find('.fa-angle-up').removeClass('fa-angle-up').addClass('fa-angle-down');
    });


    $('[data-wpel-target="_blank"]').attr( "target", "_blank" );


  });

  var url = "http://api.worldweatheronline.com/free/v1/weather.ashx?q=Orlando&format=json&num_of_days=1&key=3q84jht2zfbtrh2ervq5dcn4&callback=?";
  $.getJSON(url, function(data)
  {
    myWeather = data;
    myWeatherCode = myWeather.data.current_condition[0].weatherCode;
    myWeatherDesc = myWeather.data.current_condition[0].weatherDesc[0].value;
    myTemp = myWeather.data.current_condition[0].temp_F;

    $('.weather-icon').addClass('weather-' + myWeatherCode);
    // $('.weather-info').html(myTemp + ' | ' + myWeatherDesc);
    $('.weather-info').html(myTemp + ' &deg;');
  });

$(document).ready(function()
  {
    $("html").resize();
  });

  //<!-- Google custom search -->
  (function()
  {
    var cx = '012911231182272186646:vbggetytuoe';
    var gcse = document.createElement('script');
    gcse.type = 'text/javascript';
    gcse.async = false;
    gcse.src = (document.location.protocol === 'https' ? 'https:' : 'http:') + '//www.google.com/cse/cse.js?cx=' + cx;
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(gcse, s);
  })();



  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-10415375-1']);
  _gaq.push(['_setDomainName', 'downtownorlando.com']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script');
    ga.type = 'text/javascript';
    ga.async = true;
    ga.src = ('https:' === document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(ga, s);
  })();

</script>

