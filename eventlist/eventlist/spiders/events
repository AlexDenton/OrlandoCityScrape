    SELECT meta_value, post_id
    FROM wp_postmeta
    WHERE meta_key = 'ev_start_date'
    AND STR_TO_DATE(meta_value,'%W, %M %D %Y')
    BETWEEN STR_TO_DATE('Saturday, June 6, 2015','%W, %M %D %Y') AND STR_TO_DATE('Monday, July 6, 2015','%W, %M %D %Y')
    ORDER BY STR_TO_DATE(meta_value,'%W, %M %D %Y') DESC
  ) as t, wp_posts
    WHERE post_id = ID
    AND post_status = 'publish'
  ORDER BY STR_TO_DATE(t.meta_value,'%W, %M %D %Y') ASC
