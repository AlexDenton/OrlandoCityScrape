  $(document).ready(function() {

    var isClicked = 0;

    $('.searchButtonSpan').click(function() {
      if (isClicked == 0)
      {
        $('.hideSearch').css('visibility', 'visible');
        //$('.hideSearch').css('width', '150px');
        // $('.hideSearch').animate({width: 500}, 250);
        $('.hideSearch').animate({width: 600}, 250);
        $( "input.hideSearch" ).select();
        isClicked = 1;
      }
      else
      {
        $('.hideSearch').animate({width: 0}, 250, function() {
          $('.hideSearch').css('visibility', 'hidden');
        });
        isClicked = 0;
      }
    });

    $('.hideSearch').keyup(function(e) {
      if (e.keyCode == 13) {
        //window.location = "/commercialrealestate/properties/?location="+$(this).val();
      }
    });
  });

  $(function() {
    $('.parking-box').bind( 'click', function() {
      window.open('//itunes.apple.com/us/app/orlando-parkin/id391522969?mt=8');
    });
  });

  $(function() {
    $('.header-wrapper').clingify({
      breakpoint: 0, // in pixels
      extraClass: '',
      throttle: 100, // in milliseconds

      // Callback functions:
      detached: $.noop,
      locked: $.noop,
      resized: $.noop
    });
  });

